{
    "name": "grundic-teamcity",
    "version": "2.0.0",
    "author": "grundic",
    "summary": "Puppet module to install and configure Teamcity",
    "license": "Apache-2.0",
    "source": "https://github.com/grundic/puppet-teamcity",
    "project_page": "https://github.com/grundic/puppet-teamcity",
    "issues_url": "https://github.com/grundic/puppet-teamcity/issues",
    "tags": [
      "TeamCity",
      "JetBrains",
      "CI",
      "Continues Integration"
    ],
    "operatingsystem_support": [
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "4",
        "5",
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "4",
        "5",
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "6",
        "7",
        "8"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "12.04",
        "14.04",
        "15.10"
      ]
    },
    {
      "operatingsystem": "Windows",
      "operatingsystemrelease": [
        "Server 2003",
        "Server 2003 R2",
        "Server 2008",
        "Server 2008 R2",
        "Server 2012",
        "Server 2012 R2",
        "7",
        "8"
      ]
    }
    ],
    "dependencies": [
        {
            "name": "puppetlabs-stdlib",
            "version_requirement": ">= 2.3.0"
        },
        {
            "name": "maestrodev-wget",
            "version_requirement": ">= 1.0.0"
        },
        {
            "name": "puppet-download_file",
            "version_requirement": ">= 1.2.1"
        },
        {
            "name": "puppetlabs-inifile",
            "version_requirement": ">= 1.4.1"
        },
        {
            "name": "puppet-windows_env",
            "version_requirement": ">= 2.2.2"
        }
    ]
}
